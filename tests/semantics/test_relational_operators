input: |+
  <script type="text/JavaScript">
    document.write(1 <  1,  "<br />");
    document.write(1 <  0,  "<br />");
    document.write(1 <  2,  "<br />");

    document.write(1 <= 1,  "<br />");
    document.write(1 <= 0,  "<br />");
    document.write(1 <= 2,  "<br />");

    document.write(1 >  1,  "<br />");
    document.write(1 >  0,  "<br />");
    document.write(1 >  2,  "<br />");

    document.write(1 >= 1,  "<br />");
    document.write(1 >= 0,  "<br />");
    document.write(1 >= 2,  "<br />");

  </script>
